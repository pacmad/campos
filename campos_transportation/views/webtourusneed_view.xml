<?xml version="1.0"?>
<openerp>
<data>
<record id="view_tree_webtourusneed" model="ir.ui.view">
<field name="name">Webtour Us Need</field>
<field name="model">campos.webtourusneed</field>
<field name="arch" type="xml">
<tree>
<field name="id"/>
<field name="participant_id"/>
<field name="campos_TripType_id"/>
<field name="travelgroup"/>
<field name="campos_startdatetime"/>
<field name="campos_startdestinationidno"/>
<field name="campos_enddestinationidno"/>
<field name="campos_writeseq"/>
<field name="campos_transferedseq"/>
<field name="campos_transfered"/>
<field name="campos_demandneeded"/>
<field name="webtour_needidno"/>
</tree>
</field>
</record>
<record id="view_form_webtourusneed" model="ir.ui.view">
<field name="name">Webtour Us Need</field>
<field name="model">campos.webtourusneed</field>
<field name="arch" type="xml">
<form>
<header>
<button name="get_create_webtour_need" type="object" string="Update Webtour" class="oe_highlight" />
<button name="updatewebtourtofromusneeds" type="object" string="Update Webtour usNeed" class="oe_highlight" />
<button name="updateusneedtriptypes" type="object" string="Set Trip Types on all usNeeds" class="oe_highlight" />
</header>
<sheet>
<group name="user_top">
<field name="participant_id" readonly="0"/>
<field name="campos_TripType_id"/>
<field name="travelgroup"/>
<group name="group_left">
<field name="campos_startdatetime" readonly="0"/>
<field name="campos_startdestinationidno" readonly="0"/>
<field name="campos_startnote" readonly="0"/>
<field name="campos_enddatetime" readonly="0"/>
<field name="campos_enddestinationidno" readonly="0"/>
<field name="campos_endnote" readonly="0"/>
<field name="campos_writeseq"/>
<field name="campos_transferedseq"/>
<field name="campos_demandneeded"/>
<field name="travelneed_id"/>
</group>
<group name="group_right">
<field name="webtour_startdatetime" readonly="1"/>
<field name="webtour_startdestinationidno" readonly="1"/>
<field name="webtour_startnote" readonly="1"/>
<field name="webtour_enddatetime" readonly="1"/>
<field name="webtour_enddestinationidno" readonly="1"/>
<field name="webtour_endnote" readonly="1"/>
<field name="webtour_useridno" readonly="1"/>
<field name="webtour_groupidno" readonly="1"/>
<field name="webtour_needidno" readonly="1"/>
<field name="webtour_touridno" readonly="1"/>
<field name="webtour_CurrentDateTime" readonly="1"/>

</group>
</group>
</sheet>
</form>
</field>
</record>

<record id="view_tree_WebtourTripType" model="ir.ui.view">
	<field name="name">Webtour Trip Type</field>
	<field name="model">campos.webtourusneed.triptype</field>
	<field name="arch" type="xml">
		<tree>
			<field name="name"/>	
		</tree>
	</field>
</record> 

<record id="view_form_WebtourTripType" model="ir.ui.view">
	<field name="name">Webtour Trip Type</field>
	<field name="model">campos.webtourusneed.triptype</field>
	<field name="arch" type="xml">
		<form>
		<sheet>
			<field name="name"/>
			<field name="traveldate_ids" widget="one2many_list" nolabel="0" string="Travel Dates">
				<tree editable="bottom">
					<field name="name" widget="date"/>
				</tree>
			</field>
		</sheet>	
		</form>
	</field>
</record> 

<record id="view_tree_WebtourTripOverview" model="ir.ui.view">
	<field name="name">Webtour Trip Overview</field>
	<field name="model">campos.webtourusneed.overview</field>
	<field name="arch" type="xml">
		<tree>
	    <field name="registration_id" />
    	<field name="webtour_groupidno" />
	    <field name="campos_TripType_id" /> 
	    <field name="travelgroup" />	    
    	<field name="campos_startdatetime" />
	    <field name="campos_startdestinationidno" />
    	<field name="campos_enddestinationidno" />
    	<field name="pax" /> 
    	<field name="excessdemand" />
    	<field name="nottransfered" />
    	<field name="notdeleted" />
    	<field name="startdestdiffer" />
    	<field name="enddestdiffer"/>
    	<field name="startdatetimediffer" />
    	<field name="enddatetimediffer" />
    	<field name="startnotediffer" />    
    	<field name="endnotediffer" />		
		</tree>
	</field>
</record> 


<!-- Action to open Webtour Us Need list -->
<act_window id="action_webtourusneed"
name="Camp OS Webtour Us Need"
res_model="campos.webtourusneed"
view_mode="tree,form" />

<act_window id="action_WebtourTripType"
name="Camp OS Webtour Trip Type"
res_model="campos.webtourusneed.triptype"
view_mode="tree,form" />

<act_window id="action_WebtourTripTypeDate"
name="Camp OS Webtour Trip Type Date"
res_model="campos.webtourusneed.triptype.date"
view_mode="tree,form" />

<act_window id="action_WebtourOverview"
name="Camp OS Webtour Trip Overview"
res_model="campos.webtourusneed.overview"
view_mode="tree,form" />

<!-- Menu item to open Campos Webtour usNeed list  -->
<menuitem id="menu_action_campos_transportation_usneed" parent="menu_campos_transportation" name="Webtour usNeed" action="action_webtourusneed" groups="campos_event.group_campos_admin" sequence="30"/>
<menuitem id="menu_action_campos_WebtourTripType" parent="menu_campos_transportation" name="WebtourTripType" action="action_WebtourTripType" groups="campos_event.group_campos_admin" sequence="55"/>
<menuitem id="menu_action_campos_WebtourTripTypeDate" parent="menu_campos_transportation" name="WebtourTripTypeDate" action="action_WebtourTripTypeDate" groups="campos_event.group_campos_admin" sequence="56"/>
<menuitem id="menu_action_campos_WebtourOverview" parent="menu_campos_transportation" name="WebtourTripTypeOverview" action="action_WebtourOverview" groups="campos_event.group_campos_admin" sequence="57"/>

</data>
</openerp>
