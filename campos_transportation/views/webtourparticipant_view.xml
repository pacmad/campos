<?xml version="1.0"?>
<openerp>
<data>
<record id="view_tree_webtourparticipant" model="ir.ui.view">
<field name="name">Webtour Participant</field>
<field name="model">campos.event.participant</field>
<field name="inherit_id" ref="campos_event.campos_event_participant_form2"/>
<field name="arch" type="xml">
<tree>
<field name="webtourusgroupidno"/>
<field name="webtourususeridno"/>
<field name="usecamptransporttocamp"/>
<field name="usecamptransportfromcamp"/>
<field name="scoutgroup"/>
</tree>
</field>
</record>
<record id="view_form_webtourparticipant" model="ir.ui.view">
<field name="name">Webtour Participant</field>
<field name="model">campos.event.participant</field>
<field name="inherit_id" ref="campos_event.view_event_registration_participant_form"/>
<field name="arch" type="xml">
<form>
<sheet>
<group name="user_top">
<field name="partner_id"/>
<field name="registration_id"/>
<field name="webtourusgroupidno"/>
<field name="webtourususeridno"/>
<field name="scoutgroup"/>
<field name="tocampfromdestination_id" readonly="0"/>
<field name="fromcamptodestination_id" readonly="0"/>
<field name="tocampdate" readonly="0"/>
<field name="fromcampdate" readonly="0"/>
<field name="usecamptransporttocamp" readonly="0"/>
<field name="usecamptransportfromcamp" readonly="0"/>
<field name="tocampusneed_id" readonly="1"/>
<field name="fromcampusneed_id" readonly="1"/>
</group>
</sheet>
</form>
<header>
<button name="clearusecamptransport" type="object" string="Clear Use Transport" class="oe_highlight" />
</header>
</field>
</record>
<!-- Action to open Webtour Participant list -->
<act_window id="action_webtourparticipant"
name="Camp OS Webtour Participant"
res_model="campos.event.participant"
view_mode="tree,form" />
<!-- Menu item to open Webtour Order list -->
<menuitem id="menu _webtourparticipant"
name="Campos transportation Participant"
parent="mail.mail_feeds"
sequence="32"
action="action_webtourparticipant" />
</data>
</openerp>
