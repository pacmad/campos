		<!-- Add to Event view -->
		<record id="view_event_form" model="ir.ui.view">
			<field name="name">event.event.campos.reg.inherit2</field>
			<field name="model">event.event</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="campos_event.view_event_form" />
			<field name="arch" type="xml">
				<xpath expr="//notebook" position="inside">
					<page string="Event Days">
						<field name="event_day_ids" colspan="1" nolabel="1"  context="{'default_event_id': id}">
							<tree editable="bottom">
                <field name="event_date"/>
                <field name="event_period"/>								
           		</tree>
           	</field>
          </page>
        </xpath>
			</field>	
		</record>
		

		
		<record id="view_form_finalregistration_participant" model="ir.ui.view">
			<field name="name">Final Registration Participant Form</field>
			<field name="model">campos.event.participant</field>
			<field name="priority">17</field>
			<field name="arch" type="xml">
				<form>
					<header>
							<button name="inactivate_participant" 
											type="object"    
											string="Inactivate Participant" class = "oe_edit_only" attrs="{'invisible': [('state', '=', 'deregistered')]}" />
							<button name="activate_participant" 
											type="object"    
											string="Re-activate Participant" class = "oe_edit_only" attrs="{'invisible': [('state', '!=', 'deregistered')]}" />
							<field name="state" widget="statusbar" statusbar_visible="draft,deregistered" statusbar_colors="{'cancel':'red'}"/>
					</header>
					<sheet>
						<group name="sheet2">
							<field name="registration_id" invisible="1"/>
							<field name="participant" invisible="1"/>
							<field name="parent_id" invisible="1"/>
							<field name="participant_number" />
							<field name="name"/>
							<field name="birthdate" required="1" options="{'datepicker': {'yearRange': 'c-100:c+0'}}"/>
						</group>
						<notebook name="top">
							<page name="contact" string="Contact info">
								<group>
									<field name="email"/>
									<field name="mobile"/>
									<field name="street"/>
									<field name="street2"/>
									<field name="zip"/>
									<field name="city"/>
									<field name="country_id"/>
									<field name="access_token_id" invisible="1"/>
								</group>
							</page>
						</notebook>
						<group>
							<separator string="Transport" colspan="2"/>
							<field name="transport_to_camp"/>
							<field name="transport_from_camp"/>
							<separator string="Participation" colspan="2"/>
							<button name="check_all_maincamp_days" 
											type="object"    
											string="Check all days" class = "oe_edit_only"/>
							<button name="uncheck_all_days" 
											type="object"    
											string="Un-check all days" class = "oe_edit_only"/> 
							<button name="check_all_first_half_days" 
											type="object"    
											string="Saturday to Wednesday" class = "oe_edit_only"/>
							<button name="check_all_second_half_days" 
											type="object"    
											string="Wednesday to Sunday" class = "oe_edit_only"/>
							<field colspan="2" name="camp_day_ids" widget="one2many_list" nolabel="1">
								<tree delete="false" create="false" editable="bottom">
									<field name="the_date" readonly="1"/>
									<field name="day_id" invisible="1"/>
									<field name="will_participate"/>
								</tree>
							</field>

						</group>
					</sheet>
				</form> 
			</field>
		</record>
		
